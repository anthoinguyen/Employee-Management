                                      <option value="Thái"> </option>
                                      <option value="Mường"> </option>
                                      <option value="Khơ me"> </option>
                                    </datalist>
                                  </div>
                                </div>
                              </div>
                              <div class="col-lg-6">
                                <div class="form-group">
                                  <label for="" class="col-sm-6 control-label"
                                    >Tôn giáo</label
                                  >
                                  <div class="col-sm-12">
                                    <input
                                      type="text"
                                      class="form-control"
                                      id="tongiao2"
                                      value="<%- user.religion %>"
                                      list="tongiao"
                                    />
                                    <datalist id="tongiao">
                                      <option value="Không"> </option>
                                      <option value="Phật Giáo"> </option>
                                      <option value="Công Giáo"> </option>
                                      <option value="Hòa Hảo"> </option>
                                      <option value="Cao Đài"> </option>
                                      <option value="Tin Lành"> </option>
                                      <option value="Hồi Giáo"> </option>
                                    </datalist>
                                  </div>
                                </div>
                              </div>
                            </div>

                            <div class="row">
                              <div class="col-lg-12">
                                <div class="form-group">
                                  <label for="" class="col-sm-12 control-label"
                                    >Nơi đăng ký hộ khẩu thường trú</label
                                  >
                                  <div class="col-sm-12">
                                    <input
                                      id="HoKhau"
