        }
        let temp = {
          userImg: $("#editFile")[0].files[0]
        };
        $.ajax({
          method: "PUT",
          url: "/thong-tin-ca-nhan/hinh-anh",
          data: formdata,
          contentType: false,
          processData: false,
          success: function(res) {
            if (res.success) {
              $(".images_upload").append(
                '<img src="' +
                  res.data.userImage +
                  '" style="max-height: 180px; width: 120px; margin:5px 0px; border: 1px solid black; border-radius: 10px">'
              );
              alert(res.message);
            } else {
