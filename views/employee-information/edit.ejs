                                        />
                                      </div>
                                      <div class="col-sm-4">
                                        <input
                                          id="quequanHuyen"
                                          type="text"
                                          class="form-control"
                                          value="<%- user.hometown ? user.hometown.district : null %>"
                                        />
                                      </div>
                                      <div class="col-sm-4">
                                        <input
                                          id="quequanTinh"
                                          type="text"
                                          class="form-control"
                                          value="<%- user.hometown ? user.hometown.province : null %>"
                                        />
                                      </div>
                                    </div>
                                  </div>
                                </div>
                              </div>
                            </div>

