                            <div class="row">
                              <div class="col-lg-6">
                                <div class="form-group">
                                  <label for="" class="col-sm-12 control-label"
                                    >Trình độ giáo dục phổ thông</label
                                  >
                                  <div class="col-sm-12">
                                    <input
                                      id="trinhdoGD"
                                      type="text"
                                      value="<%- user.educationLevel ? user.educationLevel.generalEducation.whichClass : null %>"
                                      class="form-control "
                                    />
                                  </div>
                                </div>
                              </div>
                              <div class="col-lg-6">
                                <div class="form-group">
                                  <label for="" class="col-sm-12 control-label"
                                    >Trình độ chuyên môn cao nhất</label
                                  >
                                  <div class="col-sm-12">
                                    <select class="form-control" id="trinhdoCM">
                                      <option
                                        value="<%- user.educationLevel ? user.educationLevel.highestQualification : null %>"
                                      >
                                        <%- user.educationLevel ? user.educationLevel.highestQualification : null %>
                                      </option>
                                      <option value="Tiến sĩ">Tiến sĩ</option>
